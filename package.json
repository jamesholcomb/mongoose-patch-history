{
  "name": "mongoose-patch-history",
  "version": "3.1.0",
  "description": "Mongoose plugin that saves a history of JSON patch operations for all documents belonging to a schema in an associated 'patches' collection",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Christoph Werner <christoph@codepunkt.de>",
  "contributors": [
    "Robin Weinreich <robin.weinreich@posteo.de>",
    "Brett Ausmeier <brett@ausmeier.co.za>",
    "Ava Johnson <ava.johnson@zohomail.com>"
  ],
  "engines": {
    "node": ">= 22.18.0"
  },
  "license": "MIT",
  "dependencies": {
    "fast-json-patch": "^2.2.1",
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "mongoose": "^8"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/lodash": "^4.17.16",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.31.0",
    "husky": "^9.1.7",
    "mocha": "^11.2.2",
    "mongoose": "^8.21.1",
    "nyc": "^17.1.0",
    "prettier": "^3.5.3",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\" --fix",
    "coverage": "nyc mocha",
    "prepublishOnly": "npm run build",
    "prepare": "husky && npm run build",
    "prettier": "prettier --no-semi --single-quote --trailing-comma none --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "mocha \"test/**/*.test.ts\""
  },
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/gonsfx/mongoose-patch-history"
  },
  "bugs": "https://github.com/gonsfx/mongoose-patch-history/issues",
  "keywords": [
    "mongo",
    "Mongo",
    "MONGO",
    "mongodb",
    "Mongodb",
    "MONGODB",
    "mongoose",
    "Mongoose",
    "MONGOOSE",
    "plugin",
    "Plugin",
    "PLUGIN",
    "history",
    "History",
    "HISTORY",
    "versioning",
    "Versioning",
    "VERSIONING",
    "rollback",
    "Rollback",
    "ROLLBACK",
    "json",
    "Json",
    "JSON",
    "jsonpatch",
    "JsonPatch",
    "JSONPATCH",
    "json-patch",
    "Json-Patch",
    "JSON-PATCH",
    "json patch",
    "Json Patch",
    "JSON PATCH"
  ]
}
